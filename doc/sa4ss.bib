title = {{Depth and habitat distributions of three species of rockfish (\emph{Sebastes}) in British Columbia: observations from the submersible PISCES IV}},
volume = {17},
year = {1986}
}
@article{Hightower1996,
abstract = {I used simulations based on a trawl fishery for widow rockfish (Sebastes entomelas) to compare the reliability of catch-at-age analyses using auxiliary survey estimates of either relative or absolute recruitment. Survey estimates of recruitment were assumed to have a coefficient of variation (CV) of either 0, 25, or 50%. Catch-at-age analyses were done in years 8-20 of a 20-year management horizon, on the basis of 7-19 years of catch-at-age and survey data. Errors in estimated stock size were autocorrelated and were often large in years 8-12 but decreased considerably once about 12-15 years of data were available. For survey CVs of 25-50%, a precise (±20%) estimate of stock size was about twice as likely using survey estimates of absolute abundance than of relative abundance. Frequently, similar results were obtained for imprecise, absolute abundance surveys (CV = 50%) and more precise, relative abundance surveys (CV = 25%). Because of larger errors in estimating abundance and setting catch quotas, population trajectories varied over a wider range when a relative abundance survey was used. In one case (target fishing mortality rate of 0.25, survey CV of 25%), linear trends in biomass were estimated reliably using either relative or absolute abundance data.},
author = {Hightower, Joseph E.},
doi = {10.1139/f95-168},
file = {::},
issn = {0706652X},
journal = {Canadian Journal of Fisheries and Aquatic Sciences},
number = {1},
pages = {70--79},
title = {{Comparison of reliability of catch-at-age analyses using auxiliary survey estimates of relative or absolute recruitment}},
volume = {53},
year = {1996}
}
@techreport{Zimmermann2001,
abstract = {A review of the National Marine Fisheries Service U.S. West Coast triennial bottom trawl survey time series (1977-98) revealed a large number of hauls with unusually small catches, particularly during the first three triennial surveys (1977, 1980, and 1983). Beginning in 1986, technological advances in the equipment that we use to monitor trawl performance have progressively improved our understanding of how our sampling trawl operates. This knowledge has led to a subtle evolution of trawling procedures and probably increased the rate at which we catch benthic fish and invertebrates, making year-to-year comparisons of our survey data more challenging.A minimum catch per unit effort value of a benthic species group (CPUEB ), derived from the most recent and most technologically advanced survey in 1998, was used as a criterion to eliminate trawl hauls with poor bottom contact from earlier surveys. The truncated data sets produce significantly larger biomass estimates, especially in 1980 where 156 of 505 hauls (30.9%) were removed, with increases of 40% for Dover sole (Microstomus pacificus), 42% for petrale sole (Eopsetta jordani), and 55% for Pacific sanddab (Citharichthys sordidus). These new estimates are not corrections to standard survey results. Rather, they are most likely under- estimates of the magnitude of how this new interpretation could change the original biomass estimates and abundance trends. Additionally, the CPUEB across each earlier survey was significantly less than in the 1998 survey, indicating that the impact was not just limited to a specific group of hauls with unusually low catches. Total catch of flatfish has also changed substantially during the time series, with the highest catches of flatfish occurring in the recent surveys, and the lowest catches occurring in the earlier surveys. While a switch from a nylon net to one constructed of polyethylene in 1986, and an unusual occurrence of jellyfish schools in 1980 did not significantly affect the CPUEB, there were significant differences associated with different vessel-years. This is further evidence that changes in the CPUEB over the time series is not related to a natural change in the environment or caused by gear differences, but may be linked to changing fishing methods.},
author = {Zimmermann, Mark and Wilkins, Mark E and Weinberg, Ken L and Lauth, Robert R and Shaw, Frank R},
booktitle = {NOAA Tech. Memo. NMFS AFSC},
file = {::},
number = {June},
pages = {140},
title = {{Retrospective analysis of suspiciously small catches in the National Marine Fisheries Service west coast triennial bottom trawl survey}},
volume = {03},
year = {2001}
}
