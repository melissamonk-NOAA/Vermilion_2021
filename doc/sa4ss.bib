journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Ecosystem services,Ecosystem-based management,Energy technology,Sebastes,Secondary production},
number = {43},
pages = {15462--15467},
pmid = {25313050},
title = {{Oil platforms off California are among the most productive marine fish habitats globally}},
volume = {111},
year = {2014}
}
